{
  "name": "covid19-app-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server",
    "dev-server": "nodemon server",
    "client-install": "cd client/covid19-app && npm install",
    "client": "cd client/covid19-app && npm start",
    "dev": "concurrently \"npm run dev-server\" \"npm run client\"",
    "build": "cd client/covid19-app && npm install && npm run build"
  },
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^6.0.0",
    "express": "^4.16.4"
  }
}
